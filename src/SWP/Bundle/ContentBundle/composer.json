{
  "name": "swp/content-bundle",
  "type": "symfony-bundle",
  "description": "Articles, media and routes handing in Superdesk Publisher.",
  "keywords": ["content", "articles", "superdesk", "gimme", "gimmelist", "webpublisher", "twig"],
  "license":"AGPL-3.0-only",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Paweł Mikołajczuk",
      "email": "pawel.mikolajczuk@sourcefabric.org"
    },
    {
      "name": "Rafał Muszyński",
      "email": "rafal.muszynski@sourcefabric.org"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../../*/*"
    }
  ],
  "require": {
    "php": "^7.3",
    "swp/templates-system-bundle": "^2.0",
    "swp/bridge-bundle": "^2.0",
    "swp/jms-serializer-bridge": "^2.0",
    "swp/rule": "^2.0",
    "symfony/validator": "^4.2",
    "symfony/form": "^4.2",
    "symfony/css-selector": "^4.2",
    "symfony/security-bundle": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/doctrine-bridge": "<4.2.5",
    "symfony/dom-crawler": "^4.2",
    "symfony/event-dispatcher": "^4.2",
    "symfony/property-access": "^4.2",
    "symfony/filesystem": "^4.2",
    "symfony/config": "^4.2",
    "symfony/yaml": "^4.2",
    "symfony/options-resolver": "^4.2",
    "symfony-cmf/core-bundle": "^2.0",
    "symfony-cmf/routing-bundle": "^2.0",
    "stof/doctrine-extensions-bundle": "^1.3",
    "friendsofsymfony/rest-bundle": "^2.5@stable",
    "knplabs/knp-components": "^1.3",
    "oneup/flysystem-bundle": "^3.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "nelmio/api-doc-bundle": "^3.0",
    "willdurand/hateoas-bundle": "^2.0",
    "hoa/mime": "3.16.01.15",
    "phpoption/phpoption": "1.5.*",
    "burgov/key-value-form-bundle": "@stable",
    "litipk/flysystem-fallback-adapter": "dev-master",
    "zendframework/zend-stdlib": "^3.2@dev",
    "symfony/translation": "^4.2",
    "swp/seo-bundle": "^2.0",
    "symfony/templating": "^4.2",
    "sentry/sentry-symfony": "^3.0",
    "swp/redirect-route-bundle": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpspec/phpspec": "^5.1",
    "phpunit/phpunit": "^7.0",
    "symfony/framework-bundle": "^4.2",
    "symfony/http-kernel": "^4.2 <4.4",
    "symfony/twig-bridge": "^4.2",
    "symfony/expression-language": "^4.2",
    "symfony/twig-bundle": "^4.2",
    "symfony/debug": "^4.2 <4.4",
    "symfony/monolog-bridge": "^4.2",
    "symfony/stopwatch": "^4.2",
    "symfony/var-dumper": "^4.2",
    "doctrine/doctrine-bundle": "~1.8",
    "doctrine/data-fixtures": "^1.3",
    "doctrine/doctrine-cache-bundle": "1.3.*",
    "liip/functional-test-bundle": "^2.0 <2.0.0-alpha16",
    "doctrine/doctrine-fixtures-bundle": "^3.0.0",
    "theofidry/alice-data-fixtures": "^1.0",
    "nelmio/alice": "3.5.*",
    "sensio/framework-extra-bundle": "^5.0",
    "asm89/twig-cache-extension": "^1.3",
    "doctrine/orm": "^2.6"
  },
  "autoload": {
    "exclude-from-classmap": [
      "Tests/",
      "spec/"
    ],
    "psr-4": { "SWP\\Bundle\\ContentBundle\\": "" }
  },
  "autoload-dev": {
    "psr-4": {
      "SWP\\Bundle\\ContentBundle\\Tests\\": "Tests/",
      "SWP\\Bundle\\ContentBundle\\spec\\": "spec/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1-dev"
    }
  }
}
