{
  "name": "swp/content-bundle",
  "type": "symfony-bundle",
  "description": "Articles, media and routes handing in Superdesk Publisher.",
  "keywords": ["content", "articles", "superdesk", "gimme", "gimmelist", "webpublisher", "twig"],
  "license":"AGPL-3.0",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Paweł Mikołajczuk",
      "email": "pawel.mikolajczuk@sourcefabric.org"
    },
    {
      "name": "Rafał Muszyński",
      "email": "rafal.muszynski@sourcefabric.org"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../*"
    },
    {
      "type": "path",
      "url": "../../Component/*"
    },
    {
      "type": "path",
      "url": "../../Bridge/*"
    }
  ],
  "require": {
    "php": "^7.0",
    "swp/templates-system-bundle": "^0.6",
    "swp/bridge-bundle": "^1.0",
    "swp/jms-serializer-bridge": "^0.5",
    "symfony/validator": "^3.1",
    "symfony/form": "^3.1",
    "symfony/css-selector": "^3.1",
    "symfony/security-bundle": "^3.1",
    "symfony/dependency-injection": "^3.1",
    "symfony/doctrine-bridge": "^3.1",
    "symfony/dom-crawler": "^3.1",
    "symfony/event-dispatcher": "^3.1",
    "symfony/property-access": "^3.1",
    "symfony/filesystem": "^3.1",
    "symfony/config": "^3.1",
    "symfony/yaml": "^3.1",
    "symfony/options-resolver": "^3.1",
    "symfony-cmf/core-bundle": "2.0.*",
    "symfony-cmf/routing-bundle": "2.0.*",
    "stof/doctrine-extensions-bundle": "1.2.x",
    "gedmo/doctrine-extensions": ">=2.4.26 <3.0",
    "friendsofsymfony/rest-bundle": "2.1.x-dev as 1.5",
    "knplabs/knp-components": "1.3.*",
    "jms/serializer-bundle": "1.2.*",
    "jms/metadata": "1.6.*",
    "oneup/flysystem-bundle": "^1.11",
    "nelmio/api-doc-bundle": "2.13.*",
    "willdurand/hateoas-bundle": "^1.2",
    "hoa/mime": "3.16.01.15",
    "phpoption/phpoption": "1.5.*"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.4",
    "phpunit/phpunit": "^5.6",
    "symfony/framework-bundle": "^3.1",
    "symfony/http-kernel": "^3.1",
    "symfony/twig-bridge": ">=3.1.7 <3.2",
    "symfony/expression-language": "^3.1",
    "symfony/twig-bundle": "^3.1",
    "symfony/monolog-bridge": "^3.1",
    "doctrine/orm": "2.6.*",
    "doctrine/doctrine-bundle": ">=1.6.7 <1.7.0",
    "doctrine/doctrine-fixtures-bundle": "2.3.*",
    "doctrine/data-fixtures": "1.3.*",
    "doctrine/doctrine-cache-bundle": "1.3.*",
    "liip/functional-test-bundle": "1.6.*",
    "nelmio/alice": "2.2.*",
    "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
    "asm89/twig-cache-extension": "1.3.0"
  },
  "autoload": {
    "exclude-from-classmap": [
      "Tests/",
      "spec/"
    ],
    "psr-4": { "SWP\\Bundle\\ContentBundle\\": "" }
  },
  "autoload-dev": {
    "psr-4": {
      "SWP\\Bundle\\ContentBundle\\Tests\\": "Tests/",
      "SWP\\Bundle\\ContentBundle\\spec\\": "spec/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
