                        "attribute": "data-sponsored"
                    },
                    "related.url" : {
                        "type" : "string",
                        "selector" : "a",
                        "attribute": "href"
                    }
                }
            },
            {
                "class": "InstantArticleRule",
                "selector" : "head",
                "properties" : {
                    "article.canonical" : {
                        "type" : "string",
                        "selector" : "link",
                        "attribute": "href"
                    },
                    "article.charset" : {
                        "type" : "xpath",
                        "selector" : "//meta[@charset]",
                        "attribute": "charset"
                    },
                    "article.markup.version" : {
                        "type" : "xpath",
                        "selector" : "//meta[@property='op:markup_version']",
                        "attribute": "content"
                    },
                    "article.auto.ad" : {
                        "type" : "xpath",
                        "selector" : "//meta[@property='fb:use_automatic_ad_placement']",
                        "attribute": "content"
                    }

                }
            },
            {
                "class": "HeaderTitleRule",
                "selector" : "h1"
            },
            {
