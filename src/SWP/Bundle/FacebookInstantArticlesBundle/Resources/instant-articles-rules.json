                    "article.time" : {
                        "type" : "string",
                        "selector" : "time",
                        "attribute": "datetime"
                    },
                    "article.datetype": {
                        "type": "constant",
                        "value": "op-modified"
                    }
                }
            },
            {
                "class": "TimeRule",
                "selector" : "time.op-published",
                "properties" : {
                    "article.time" : {
                        "type" : "string",
