html{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}*,:before,:after{-webkit-box-sizing:inherit;-moz-box-sizing:inherit;box-sizing:inherit}body{font-size:16px;font-family:Helvetica,Arial,sans-serif;color:#4c4e4d;-webkit-font-smoothing:antialiased;font-smoothing:antialiased;text-rendering:optimizeLegibility;margin:0}header,footer{padding:20px 10px}footer{font-size:.75em;text-align:center}.header-link{display:block;text-align:center}pre{word-wrap:break-word;font-family:monospace, serif;font-size:1em}p{line-height:1.6}p,blockquote{line-height:1.5}h1,h2,h3,h4,h5{margin:0}a,a:link,a:visited{text-decoration:none}a:hover,a:active,a:focus{text-decoration:underline}h1{font-size:2.1em;line-height:1.3;padding:.5em 0 0}h2{font-size:1.4em;color:#bbb;padding-bottom:.25em}h3,h4,h5{padding:.5em 0}blockquote{line-height:1.6;margin:1.5em}q:before,q:after{content:""}article[role=article]{max-width:620px;margin:0 auto}section{padding:20px}@font-face{font-family:Helvetica,Arial,sans-serif;font-weight:400;font-style:normal}body header{background:#2C3E50}body footer{background-color:#2C3E50}body a,body a:link,body a:visited{color:#fff}body h3{font-size:1.1em}body h4{font-size:1em}body h5{font-size:.75em}body blockquote{padding:10.5px 21px;border-left:5px solid #ecf0f1}body q{font-style:italic;display:block;font-size:1.4em;line-height:1.25;text-transform:uppercase;color:#6f96a8;border-top:5px solid #e0dedf;border-bottom:5px solid #e0dedf;margin:1em 0;padding:.8em 0 .5em}
