{% extends 'base.html.twig' %}

{% block body %}
<div id="page" class="container">

  <!-- Content -->
  <section id="content">
  	<div class="row">
  		<article class="news_item error_page">
  			<div class="content content_text ">
				<h2>Oops... We have encountered an error.</h2>
