{
  "name": "swp/menu-bundle",
  "type": "symfony-bundle",
  "description": "KNP Menu Bundle integrations with Superdesk Publisher.",
  "keywords": ["knp menu", "menu", "superdesk", "webpublisher", "twig"],
  "license":"AGPL-3.0",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Rafał Muszyński",
      "email": "rafal.muszynski@sourcefabric.org"
    }
  ],
  "require": {
    "php": "^7.1",
    "knplabs/knp-menu-bundle": "^2.1",
    "symfony/form": "^3.4|^4.2",
    "symfony/validator": "^3.4|^4.2",
    "swp/storage": "^1.4|^2.0",
    "swp/common": "^1.4|^2.0"
  },
  "require-dev": {
    "phpspec/phpspec": ">=3.2.3 <4.0",
    "phpspec/prophecy": "1.7.*"
  },
  "autoload": {
    "psr-4": { "SWP\\Bundle\\MenuBundle\\": "" }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
